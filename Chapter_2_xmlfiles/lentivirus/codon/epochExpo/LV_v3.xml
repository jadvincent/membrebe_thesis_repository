    <yangCodonModel id="yang07" geneticCode="universal">
        <omega>
            <parameter idref="epoch.omega07"/>
        </omega>
        <kappa>
            <parameter idref="kappa"/>
        </kappa>
        <frequencyModel idref="freqModel"/>
    </yangCodonModel>

    <yangCodonModel id="yang08" geneticCode="universal">
        <omega>
            <parameter idref="epoch.omega08"/>
        </omega>
        <kappa>
            <parameter idref="kappa"/>
        </kappa>
        <frequencyModel idref="freqModel"/>
    </yangCodonModel>

    <yangCodonModel id="yang09" geneticCode="universal">
        <omega>
            <parameter idref="epoch.omega09"/>
        </omega>
        <kappa>
            <parameter idref="kappa"/>
        </kappa>
        <frequencyModel idref="freqModel"/>
    </yangCodonModel>

	<compoundParameter id="epoch.omegas">
		<parameter idref="epoch.omega01"/>
		<parameter idref="epoch.omega02"/>
		<parameter idref="epoch.omega03"/>
		<parameter idref="epoch.omega04"/>
		<parameter idref="epoch.omega05"/>
		<parameter idref="epoch.omega06"/>
		<parameter idref="epoch.omega07"/>
		<parameter idref="epoch.omega08"/>
		<parameter idref="epoch.omega09"/>
	</compoundParameter>

    <!-- Epoch Model -->
     <epochBranchModel id="epochModel">
         <epoch id="epoch01" transitionTime="0.00001">
         	<yangCodonModel idref="yang01"/>
         </epoch>
         <epoch id="epoch02" transitionTime="0.0001">
         	<yangCodonModel idref="yang02"/>
         </epoch>
         <epoch id="epoch03" transitionTime="0.001">
         	<yangCodonModel idref="yang03"/>
         </epoch>
         <epoch id="epoch04" transitionTime="0.01">
         	<yangCodonModel idref="yang04"/>
         </epoch>
         <epoch id="epoch05" transitionTime="0.1">
         	<yangCodonModel idref="yang05"/>
         </epoch>
         <epoch id="epoch06" transitionTime="1.0">
         	<yangCodonModel idref="yang06"/>
         </epoch>
         <epoch id="epoch07" transitionTime="10">
         	<yangCodonModel idref="yang07"/>
         </epoch>
         <epoch id="epoch08" transitionTime="100">
         	<yangCodonModel idref="yang08"/>
         </epoch>

         <yangCodonModel idref="yang09"/>

         <treeModel idref="treeModel"/>
     </epochBranchModel>

	<!-- site model                                                              -->
	<siteModel id="siteModel">
		<branchSubstitutionModel> 
             <epochBranchModel idref="epochModel"/>
		</branchSubstitutionModel> 
	     <gammaShape gammaCategories="4">
			<parameter id="siteModel.alpha" value="0.5" lower="0.0"/>
		</gammaShape>
	</siteModel>

	<!-- Likelihood for tree given sequence data                                 -->
	<treeLikelihood id="treeLikelihood">
		<patterns idref="patterns"/>
		<treeModel idref="treeModel"/>
		<siteModel idref="siteModel"/>
		<epochBranchModel idref="epochModel"/>
		<rateEpochBranchRates idref="epochRates"/>	
	</treeLikelihood>

	<multivariateNormalPrior id="rate.coefficients.prior">
		<data>
			<parameter idref="rate.coefficients"/>
		</data>
		<meanParameter>
			<parameter value="0 0"/>
		</meanParameter>
		<precisionParameter>
			<matrixParameter>
				<parameter value="0.1 0"/>
				<parameter value="0 0.5"/>
			</matrixParameter>
		</precisionParameter>
	</multivariateNormalPrior>

	<multivariateNormalPrior id="omega.coefficients.prior">
		<data>
			<parameter idref="omega.coefficients"/>
		</data>
		<meanParameter>
			<parameter value="0 0"/>
		</meanParameter>
		<precisionParameter>
			<matrixParameter>
				<parameter value="0.1 0"/>
				<parameter value="0 0.5"/>
			</matrixParameter>
		</precisionParameter>
	</multivariateNormalPrior>

<!-- Define operators                                                        -->
	<operators id="operators" optimizationSchedule="log">

		<scaleOperator scaleFactor="0.75" weight="3">
			<parameter idref="kappa"/>
		</scaleOperator>
		<deltaExchange delta="0.01" weight="1">
			<parameter idref="codonfrequencies"/>
		</deltaExchange>
		<scaleOperator scaleFactor="0.75" weight="0.5">
			<parameter idref="siteModel.alpha"/>
		</scaleOperator>

		<randomWalkOperator windowSize="0.01" weight="15"  autoOptimize="true">
			<parameter idref="rate.coefficients"/>
		</randomWalkOperator> 
		<randomWalkOperator windowSize="0.01" weight="15"  autoOptimize="true">
			<parameter idref="omega.coefficients"/>
		</randomWalkOperator> 

		<subtreeSlide size="1.0" gaussian="true" weight="15">
			<treeModel idref="treeModel"/>
		</subtreeSlide>
		<narrowExchange weight="15">
			<treeModel idref="treeModel"/>
		</narrowExchange>
		<wideExchange weight="3">
			<treeModel idref="treeModel"/>
		</wideExchange>
		<wilsonBalding weight="3">
			<treeModel idref="treeModel"/>
		</wilsonBalding>
		<scaleOperator scaleFactor="0.75" weight="3">
			<parameter idref="treeModel.rootHeight"/>
		</scaleOperator>
		<uniformOperator weight="30">
			<parameter idref="treeModel.internalNodeHeights"/>
		</uniformOperator>
		<scaleOperator scaleFactor="0.75" weight="3">
			<parameter idref="yule.birthRate"/>
		</scaleOperator>

	</operators>


	<mcmc id="mcmc" chainLength="25000000" autoOptimize="true">
		<posterior id="posterior">
			<prior id="prior">

				<booleanLikelihood>
					<monophylyStatistic idref="monophyly(HIVM)"/>
					<monophylyStatistic idref="monophyly(drill)"/>
				</booleanLikelihood>
				<normalPrior mean="8.5E-5" stdev="5.0E-6">
					<statistic idref="tmrca(HIVM)"/>
				</normalPrior>
				<exponentialPrior mean="0.001" offset="0.01">
					<statistic idref="tmrca(drill)"/>
				</exponentialPrior>

                <multivariateNormalPrior idref="rate.coefficients.prior"/>
                <multivariateNormalPrior idref="omega.coefficients.prior"/>

				<logNormalPrior mean="1.0" stdev="2" offset="0.0" meanInRealSpace="true">
					<parameter idref="kappa"/>
				</logNormalPrior>

 				<uniformPrior lower="0.0" upper="1.0">
					<parameter idref="codonfrequencies"/>
				</uniformPrior>

				<exponentialPrior mean="0.5" offset="0.0">
					<parameter idref="siteModel.alpha"/>
				</exponentialPrior>

				<uniformPrior lower="0.0" upper="1.0E100">
					<parameter idref="yule.birthRate"/>
				</uniformPrior>
				<speciationLikelihood idref="speciation"/>
			</prior>
			
			<likelihood id="likelihood">
				<treeLikelihood idref="treeLikelihood"/>
			</likelihood>
			
		</posterior>
		<operators idref="operators"/>

		<!-- write log to screen                                                     -->
		<log id="screenLog" logEvery="100">
		
			<column label="Posterior" dp="4" width="12">
				<posterior idref="posterior"/>
			</column>
			
			<column label="Prior" dp="4" width="12">
				<prior idref="prior"/>
			</column>
			
			<column label="Likelihood" dp="4" width="12">
				<likelihood idref="likelihood"/>
			</column>
			
			<column label="rootHeight" sf="6" width="12">
				<parameter idref="treeModel.rootHeight"/>
			</column>

<!--			
			<column label="clock.rate" sf="6" width="12">
				<parameter idref="clock.rate"/>
			</column>
-->			
            <column label="rate effects" sf="6" width="12">
                <parameter idref="rate.coefficients"/>
            </column>

            <column label="omega effects" sf="6" width="12">
                <parameter idref="omega.coefficients"/>
            </column>
		</log>

		<!-- write log to file                                                       -->
		<log id="fileLog" logEvery="1000" fileName="LV_v3_cod.log" overwrite="false">
			<posterior idref="posterior"/>
			<prior idref="prior"/>
			<likelihood idref="likelihood"/>
			<parameter idref="treeModel.rootHeight"/>
			<parameter idref="yule.birthRate"/>
	        <parameter idref="kappa"/>
			<parameter idref="siteModel.alpha"/>

			<parameter idref="rate.coefficients"/>
			<parameter idref="omega.coefficients"/>
			<parameter idref="codonfrequencies"/>
			<parameter idref="epoch.rates"/>
			<compoundParameter idref="epoch.omegas"/>
			<treeLikelihood idref="treeLikelihood"/>
			<speciationLikelihood idref="speciation"/>
		</log>

		<!-- write tree log to file                                                  -->
		<logTree id="treeFileLog" logEvery="1000" nexusFormat="true" fileName="LV_v3_cod.trees" sortTranslationTable="true">
			<treeModel idref="treeModel"/>
			<trait name="rate" tag="rate">
				<rateEpochBranchRates idref="epochRates"/>
			</trait>
			<posterior idref="posterior"/>
		</logTree>
	</mcmc>
	<report>
		<property name="timer">
			<mcmc idref="mcmc"/>
		</property>
	</report>
</beast>

